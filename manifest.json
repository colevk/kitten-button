{
	"name": "Kitten Button",
	"version": "1.0",
	"manifest_version": 2,

	"description": "Replaces all images on a page with kittens.",

	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Kittens!"
	},
	"permissions": ["tabs", "http://*/*"],
	"background": {
		"scripts": ["background.js"]
	},
    "options_page": "options.html",
    "web_accessible_resources": [
        "images/placeholder.png",
        "images/cat1.jpeg",
        "images/cat2.jpeg",
        "images/cat3.jpeg",
        "images/cat4.jpeg",
        "images/cat5.jpeg",
        "images/cat6.jpeg",
        "images/cat7.jpeg",
        "images/cat8.jpeg",
        "images/cat9.jpeg",
        "images/cat10.jpeg",
        "images/cat11.jpeg",
        "images/cat12.jpeg",
        "images/cat13.jpeg",
        "images/cat14.jpeg",
        "images/cat15.jpeg",
        "images/cat16.jpeg"
    ]
}
